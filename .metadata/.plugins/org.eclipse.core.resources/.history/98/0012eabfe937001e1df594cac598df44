package di_p;

public class Umbrella {

	String name;
	int price;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public Umbrella(String name, int price) {
		super();
		this.name = name;
		this.price = price;
	}
	public Umbrella() {
		System.out.println("Umbrella 기본 생성자");
	}
	@Override
	public String toString() {
		return "Umbrella [name=" + name + ", price=" + price + "]";
	}
	
	
	
}
