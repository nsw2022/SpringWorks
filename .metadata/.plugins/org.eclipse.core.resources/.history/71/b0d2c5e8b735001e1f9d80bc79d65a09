package bycycle;

public class MTBBycycle implements BycycleCategory {
	
	int ba;
	
	public MTBBycycle() {
		System.out.println("MTB 자전거");
	}
	
	@Override
	public void go(int ba) {
		if (ba==2) {
			System.out.println("MTB 가유");
		}else {
			System.out.println("MTB가 못가유");
		}
	}


	@Override
	public void fix() {
		System.out.println("MTB 수리해유");

	}

	@Override
	public void fall() {
		System.out.println("MTB 넘어져유");

	}


}
