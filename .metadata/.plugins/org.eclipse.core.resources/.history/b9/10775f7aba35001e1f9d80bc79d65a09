package bycycle;

public class PokaBycycle implements BycycleCategory {
	
	int ba;
	
	public PokaBycycle() {
		System.out.println("포카리 자전거 입니다.");
	}
	
	@Override
	public void go() {
		System.out.println("샤랄라 브금이 깔리며 출발.");

	}

	@Override
	public void fix() {
		System.out.println("포카리 수리해요");
	}

	@Override
	public void fall() {
		System.out.println("포카리가 넘어져유");
	}

	public int getBa() {
		return ba;
	}

	public void setBa(int ba) {
		this.ba = ba;
	}
	
	

}
