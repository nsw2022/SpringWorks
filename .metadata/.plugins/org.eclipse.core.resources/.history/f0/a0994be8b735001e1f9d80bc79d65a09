package bycycle;

public class PokaBycycle implements BycycleCategory {
	
	int ba;
	
	public PokaBycycle() {
		System.out.println("포카리 자전거 입니다.");
	}
	
	@Override
	public void go(int ba) {
		if (ba==2) {
			System.out.println("포카리가 가유");
		}else {
			System.out.println("포리가 못가유");
		}

	}

	@Override
	public void fix() {
		System.out.println("포카리 수리해요");
	}

	@Override
	public void fall() {
		System.out.println("포카리가 넘어져유");
	}

}
