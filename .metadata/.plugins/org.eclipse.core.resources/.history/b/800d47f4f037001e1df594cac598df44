package di_p;

public class ColorStu {
	String name;
	int[] score;
	int total,avg;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int[] getScore() {
		return score;
	}
	public void setScore(int[] score) {
		this.score = score;
	}
	public int getTotal() {
		return total;
	}
	public void setTotal(int total) {
		this.total = total;
	}
	public int getAvg() {
		return avg;
	}
	public void setAvg(int avg) {
		this.avg = avg;
	}
	public ColorStu(String name, int[] score, int total, int avg) {
		super();
		this.name = name;
		this.score = score;
		this.total = total;
		this.avg = avg;
	}
	
	public ColorStu() {
		
	}
	
	
	
}
