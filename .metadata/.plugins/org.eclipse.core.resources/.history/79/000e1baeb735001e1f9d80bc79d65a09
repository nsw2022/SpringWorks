package bycycle;

public class TEST {

	public static void main(String[] args) {
		
		int ba2 = 2;
		
		PokaBycycle po = new PokaBycycle();
		po.go(ba2);
		po.fall();
		po.fix();
		
		ThreeBycycle th = new ThreeBycycle();
		th.go(ba2);
		th.fall();
		th.fix();
		
		MTBBycycle mb = new MTBBycycle();
		mb.go(ba2);
		mb.fall();
		mb.fix();
		

	}

}
