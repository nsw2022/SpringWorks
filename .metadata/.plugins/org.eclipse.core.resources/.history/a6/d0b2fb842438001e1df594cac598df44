package anno_p_main;

import jakarta.annotation.Resource;

public class RCCar {
	
	String name;
	
	@Resource
	Motor mtr;
	RCWheel rw1, rw2, rw3, rw4;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	

}

class RCWheel{
	String name;
	int size;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	@Override
	public String toString() {
		return "RCWheel [name=" + name + ", size=" + size + "]";
	}
	
	
}