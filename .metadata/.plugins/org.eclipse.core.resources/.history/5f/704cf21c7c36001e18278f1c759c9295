package di_p;

import java.util.Arrays;

public class Shcool {
	// 학생을 참조로 받고
	// 학교에서 학생의 점수를 계산
	
	StudentTest[] students;
	
	int[]totalCompare;
	int[] rankCompare;
	
	
	
	public Shcool(StudentTest students, int[] totalCompare, int[] rankCompare) {
		super();
		this.students = students;
		this.totalCompare = totalCompare;
		this.rankCompare = rankCompare;
	}
	
	@Override
	public String toString() {
		return "Shcool [students=" + students + ", totalCompare=" + Arrays.toString(totalCompare) + ", rankCompare="
				+ Arrays.toString(rankCompare) + "]";
	}
	public StudentTest getStudents() {
		return students;
	}
	public void setStudents(StudentTest students) {
		this.students = students;
	}
	public int[] getTotalCompare() {
		return totalCompare;
	}
	public void setTotalCompare(int[] totalCompare) {
		this.totalCompare = totalCompare;
	}
	public int[] getRankCompare() {
		return rankCompare;
	}
	public void setRankCompare(int[] rankCompare) {
		this.rankCompare = rankCompare;
	}
	
	
	
}
