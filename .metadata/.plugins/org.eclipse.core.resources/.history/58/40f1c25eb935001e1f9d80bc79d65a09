package bycycle;

public class ThreeBycycle implements BycycleCategory {
	
	int ba;
	String bagu;
	
	public ThreeBycycle() {
		System.out.println("세발자전거입니다.");
	}
	
	@Override
	public void go(int ba) {
		if (ba==3) {
			System.out.println("세바리가 가유");
		}else {
			System.out.println("세바리가 못가유");
		}
	}

	@Override
	public void fix() {
		System.out.println("세바리 수리해유");

	}

	@Override
	public void fall() {
		System.out.println("세바리 넘어져유");

	}

	public int getBa() {
		return ba;
	}

	public void setBa(int ba) {
		this.ba = ba;
	}

	public String getBagu() {
		return bagu;
	}

	public void setBagu(String bagu) {
		this.bagu = bagu;
	}

	
	

}
