package di_p;

public class CoffeeShop {
	Coffe order_am() {
		System.out.println("order_am실행");
		
		return new Coffe("아메리카노", 1400);
	}
	

}

class Coffe{
	String name;
	int price;
	public Coffe(String name, int price) {
		super();
		this.name = name;
		this.price = price;
	}
	
	@Override
	public String toString() {
		return "Coffe [name=" + name + ", price=" + price + "]";
	}

	
}
