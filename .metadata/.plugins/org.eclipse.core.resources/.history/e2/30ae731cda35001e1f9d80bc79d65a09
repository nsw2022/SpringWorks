package di_p;

public class InMTB implements InterBiCycle{

	String name, kind = "MTB";
	Wheel wh1, wh2;
	Handle hd;
	public void setName(String name) {
		this.name = name;
	}
	public void setWh1(Wheel wh1) {
		this.wh1 = wh1;
	}
	public void setWh2(Wheel wh2) {
		this.wh2 = wh2;
	}
	public void setHd(Handle hd) {
		this.hd = hd;
	}
	@Override
	public String toString() {
		return "InMTB [kind=" + kind + ", name=" + name + ", wh1=" + wh1 + ", wh2=" + wh2 + ", hd=" + hd + "]";
	}
}


class InThree implements InterBiCycle{
	String name, kind = "세발";
	Wheel wh1, wh2, wh3;
	Handle hd;
	public void setName(String name) {
		this.name = name;
	}
	public void setWh1(Wheel wh1) {
		this.wh1 = wh1;
	}
	public void setWh2(Wheel wh2) {
		this.wh2 = wh2;
	}
	public void setWh3(Wheel wh3) {
		this.wh3 = wh3;
	}
	public void setHd(Handle hd) {
		this.hd = hd;
	}
	@Override
	public String toString() {
		return "InThree [kind=" + kind + ", name=" + name + ", wh1=" + wh1 + ", wh2=" + wh2 + ", wh3=" + wh3 + ", hd="
				+ hd + "]";
	}
	
}


class InPocari implements InterBiCycle{
	String name, kind = "포카리";
	Wheel wh1, wh2, wh3;
	Handle hd;
	Basket bk;
	
	public void setHd(Handle hd) {
		this.hd = hd;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setWh1(Wheel wh1) {
		this.wh1 = wh1;
	}
	public void setWh2(Wheel wh2) {
		this.wh2 = wh2;
	}
	public void setWh3(Wheel wh3) {
		this.wh3 = wh3;
	}
	public void setBk(Basket bk) {
		this.bk = bk;
	}
	@Override
	public String toString() {
		return "InPocari [kind=" + kind + ", name=" + name + ", wh1=" + wh1 + ", wh2=" + wh2 + ", wh3=" + wh3 + ", hd="
				+ hd + ", bk=" + bk + "]";
	}
	
	
}