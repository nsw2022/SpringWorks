package di_p;

public class Restaurant {

	// 아침 김치 점심 바나나 저녁 샐러드
	
	public Morning m_name() {
		System.out.println("Morning");
		return new Morning("김치");
	}
	public launch l_name() {
		System.out.println("launch");
		return new launch("바나나");
	}
	public dinner d_name() {
		System.out.println("dinner");
		return new dinner("샐러드");
	}
	
	public void order_menu() {
		System.out.println("재정의 되어 출력이 달리나옵니다.");
	}
	
	
	
	
	
	
	public class Morning{
		String name;

		public Morning(String name) {
			super();
			this.name = name;
		}
		
	}
	
	public class launch{
		String name;

		public launch(String name) {
			super();
			this.name = name;
		}
	}
	
	public class dinner{
		String name;

		public dinner(String name) {
			super();
			this.name = name;
		}
	}
}

